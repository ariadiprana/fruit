<!DOCTYPE html>
<html lang="en">

<head>
  <%- include ../template/head.ejs %>
</head>

<body>
    <div id="wrapper">

        <%- include ../template/navigation.ejs %>

        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Corporate</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
              <form action="/admin/corporate/add" method="post">
                <div class="col-lg-12">
                  <% if (message.length > 0) { %>
                      <div class="alert alert-danger"><%= message %></div>
                  <% } %>
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            Add Corporate
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-6">
                                    <form role="form">
                                        <div class="form-group">
                                            <label>Name</label>
                                            <input type="text" placeholder="Name" id="name" name="name" class="form-control">
                                        </div>
                                    </form>
                                </div>
                                <!-- /.col-lg-6 (nested) -->
                            </div>
                            <!-- /.row (nested) -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                    <button type="submit" class="btn btn-default">Add</button>
                    <a href="/admin/corporate/list" class="btn btn-default">Back</a>
                </div>
                <!-- /.col-lg-12 -->
              </form>
              <!-- /form -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->

    <%- include ../template/tail.ejs %>

</body>

</html>
